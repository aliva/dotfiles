[
  /* toggle line numbers with f1 */
  {
    "key": "f1",
    "command": "settings.cycle.editor.lineNumbers"
  },
  /* quick open */
  {
    "command": "workbench.action.quickOpen",
    "key": "ctrl+shift-r"
  },
  /* show commands */
  {
    "key": "ctrl+shift+a",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "ctrl+shift+p",
    "command": "-workbench.action.showCommands"
  }
]
