[
  /* toggle line numbers with f1 */
  {
    "key": "f1",
    "command": "settings.cycle.editor.lineNumbers"
  },
  /* disable shourtcuts in terminal */
  {
    "key": "ctrl+p",
    "when": "terminalFocus",
    "command": ""
  },
  {
    "key": "ctrl+f",
    "when": "terminalFocus",
    "command": ""
  },
  /* back and forward */
  {
    "key": "alt+left",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "alt+right",
    "command": "workbench.action.navigateForward"
  },
  /* terminal on mac */
  {
    "key": "ctrl+[IntlBackslash]",
    "command": "workbench.action.terminal.toggleTerminal"
  }
]
